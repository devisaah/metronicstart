<script>
    $(".filter").hide();
    $(".menu-section").click(function() {
        var filter = $(this).attr('data-filter');
        var cruds = $("." + filter);
        if($(this).hasClass("menu-section-show")) {
            $(this).removeClass("menu-section-show");
        } else {
            $(this).addClass("menu-section-show");
        }
        $.each(cruds, function(index, element){
            if($(element).hasClass("show")){
               $(element).removeClass("show");
            } else {
               $(element).addClass("show");
            }
        });
    });
</script>