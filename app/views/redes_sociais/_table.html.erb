<%- model_class = RedeSocial -%>



<div class="accordion accordion-toggle-plus" id="accordionRedeSocial">
  <div class="card">
    <div class="card-header" id="headingRedeSocial">
      <div class="card-title collapsed" data-toggle="collapse" data-target="#collapseRedeSocial" aria-expanded="false" aria-controls="collapseRedeSocial">
        <%= model_class.model_name.human(count: 2).titleize %>
      </div>
    </div>
    <div id="collapseRedeSocial" class="collapse" aria-labelledby="headingRedeSocial" data-parent="#accordionRedeSocial" style="">
      <div class="card-body">
        <%= render partial: "redes_sociais/datatable", locals: {redes_sociais: redes_sociais} %>
      </div>
    </div>
  </div>
</div>